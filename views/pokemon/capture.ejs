<head>
  <style>
    /* Add some style to the elements */
    #game-container {
      display: flex;
      flex-direction: column;
      align-items: center;
      margin: 20px;
    }

    button {
      width: 150px;
      height: 150px;
      margin: 10px;
      border: none;
      border-radius: 50%;
      cursor: pointer;
    }

    #Charmander {
      background-color: orange;
    }

    #Squirtle {
      background-color: blue;
    }

    #Bulbasaur {
      background-color: green;
    }

    img {
      width: 200px;
      height: 200px;
    }

    h1 {
      font-family: Arial, sans-serif;
      font-size: 24px;
    }
  </style>
</head>

<div id="game-container">
  <!-- Add three buttons for the player's choice -->
  <button id="Charmander">Charmander</button>
  <button id="Squirtle">Squirtle</button>
  <button id="Bulbasaur">Bulbasaur</button>
  <!-- Add two images for the player's and computer's choice -->
  <img id="player-choice" src="" alt="" />
  <img id="computer-choice" src="" alt="" />
  <!-- Add a text element for the result -->
  <h1 id="result-text"></h1>
</div>
